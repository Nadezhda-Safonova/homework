#include <iostream>
#include <string>

using namespace std;
 
int main() 
{
	int a;
	cin >> a;
	cout << a << ":";
	bool prime = true;
	for (int i = 2; i * i <= a; ++i) 
		{
			if (a % i == 0) 
				{
					prime = false;
					break;
				}
		}
 
	if (a == 1) 
		{
			cout << "Unit ";
		} 
	else if (prime) 
		{
			cout << "Prime ";
		} 
	else 
		{
			cout << "Composite ";
		}
 
 	return 0;
}
