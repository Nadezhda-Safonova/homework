//*Напишите программу проверяющую N чисел на четность. В программе гарантируется, что все числа целые и их не менее одного*/

#include <iostream>
#include <string>
 
using namespace std;
 
int main()
{
    int a;
	cin >> a;
	for (int i = 0; i < a; ++i) 
	    {
		    int b;
		    cin >> b;
		    cout << b << ":";
		    if (b % 2 == 0) 
		       {
			        cout << "Even ";
		        } 
			else 
		            {
			            cout << "Odd ";
		            }
	    }
	return 0;
 
}
